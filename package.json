  "repository": {
    "type": "git",
    "url": "https://github.com/cpro95/mycodings_fly_dev.git"
  },
  "scripts": {
    "create": "hugo new",
    "clean": "rimraf public",
    "dev": "hugo server --disableFastRender --noHTTPCache",
    "format": "prettier **/** -w -c",
    "build": "hugo --minify --gc",
    "preview": "vite preview --outDir public",
    "new:life": "node new-life.mjs",
    "new:blog": "node new-blog.mjs",
    "deploy": "./deploy.sh"
  },
  "dependencies": {
    "@tabler/icons": "^3.34.1",
    "@thulite/doks-core": "^1.8.3",
    "@thulite/images": "^3.3.1",
    "@thulite/inline-svg": "^1.2.0",
    "@thulite/seo": "^2.4.1",
    "thulite": "^2.6.3"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.5",
    "inquirer": "^12.9.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "vite": "^7.0.6"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
